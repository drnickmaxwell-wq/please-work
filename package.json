{
  "name": "smh-web-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "guard:hero": "node scripts/guard-hero-freeze.mjs",
    "guard:hero:baseline": "node scripts/guard-hero-freeze.mjs --write-baseline",
    "brand:grain": "node scripts/generate-film-grain.mjs --seed champagne-v1 --width 2560 --height 1440 --intensity 0.015",
    "freeze:update-filmgrain": "node scripts/freeze-update-filmgrain.mjs",
    "lint": "eslint .",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write public/brand/manifest.json lib/brand/manifest.ts app/brand.ts components/sections/HeroLuxury.tsx tests/brand-manifest.spec.ts",
    "test": "node --test --import tsx --import ./tests/register-css-loader.js tests/brand-manifest.spec.ts tests/preview/homepage.test.tsx",
    "brand:guard": "node scripts/brand-guard.cjs",
    "brand:canon": "node scripts/generate-champagne-canon.mjs",
    "brand:compare": "node scripts/compare-manus-to-champagne.mjs",
    "test:guard": "node scripts/brand-guard.smoke.mjs",
    "brand:guard:structure": "node scripts/brand-guard-structure.cjs",
    "brand:audit": "node scripts/compare-manus-to-champagne.mjs",
    "brand:report": "node scripts/brand-report.cjs",
    "brand:manifest": "node -e \"console.log(require('fs').readFileSync('./public/brand/champagne_machine_manifest_full.json','utf8'))\"",
    "brand:manus": "node -e \"console.log(require('fs').readFileSync('./public/brand/manus_import_unified_manifest_20251104.json','utf8'))\"",
    "verify:hue": "node scripts/verify-hue.cjs",
    "verify:brand": "pnpm exec playwright test tests/brand-lock.spec.ts",
    "verify:visual": "pnpm exec playwright test preview/brand-live.spec.ts",
    "hue:capture": "node scripts/capture-gradient.cjs",
    "hue:lock": "node scripts/lock-gradient.cjs",
    "prepush": "pnpm run brand:guard && pnpm run verify:hue && pnpm test:brand",
    "prepare": "husky",
    "test:brand": "pnpm exec playwright test tests/brand-lux.spec.ts",
    "test:brand:manifests": "playwright test tests/brand-manifests.spec.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/playfair-display": "^5.2.8",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@types/three": "^0.180.0",
    "ai": "^5.0.33",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "15.5.7",
    "next-seo": "^6.8.0",
    "openai": "^5.19.1",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "stripe": "^18.5.0",
    "three": "^0.180.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "micromatch": "^4.0.8",
    "next-sitemap": "^4.2.3",
    "postcss-import": "^16.1.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "typescript": "^5",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "sharp": "^0.33.3"
  },
  "engines": {
    "node": ">=20"
  }
}
